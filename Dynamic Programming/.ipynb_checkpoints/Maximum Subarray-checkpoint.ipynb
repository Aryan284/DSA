    "nums = [1]\n",
    "def subarray(nums):\n",
    "    max_sum = nums[0]\n",
    "    for i in range(1, len(nums)):\n",
    "        if nums[i - 1] + nums[i] > nums[i]:\n",
    "            nums[i] = nums[i - 1] + nums[i]\n",
    "        max_sum = max(max_sum, nums[i])\n",
    "    return max_sum\n",
    "subarray(nums)"
   ]
