   "source": [
    "def ngl(arr):\n",
    "    stack = []\n",
    "    res = [0] * len(arr)\n",
    "    for i in range(len(arr)):\n",
    "        while stack and stack[-1] <= arr[i]:\n",
    "            stack.pop()\n",
    "        res[i] = -1 if not stack else stack[-1]\n",
    "        stack.append(arr[i])\n",
    "    return res\n",
    "ngl([1,3,2,4])"
   ]
