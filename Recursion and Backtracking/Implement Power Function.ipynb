    "x = 0.00001\n",
    "n = 21474568\n",
    "def Exponent(base, power):\n",
    "    if power == 0:\n",
    "        return 1\n",
    "    mid = Exponent(base, power//2)\n",
    "    if power % 2 == 0:\n",
    "        return  mid * mid\n",
    "    else:\n",
    "         return base * mid * mid\n",
